var fact = function(n) {
  if (n < 2){
    return 1;
  }
    else{
    return fact(n - 1) * n;
  }
}

var fib = function(n) {
  if (n < 2){
    return 1;
  }
    else{
    return fib(n - 1) + fib(n - 2);
  }
}

var randomItem = function(n){
  return n[Math.floor(Math.random() *n.length)];
}

list = [1,2,3,4,5,6,7,8];

var randomStudent = function() {
  return randomItem(list);
}
